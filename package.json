{
	"name": "error-tab-highlighter",
	"displayName": "Error tab highlighter",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
